{
  "name": "boilerplate",
  "version": "0.3.0",
  "private": false,
  "description": "My typescript/php boiler plate.",
  "author": "Paul Thomas <@PaulDThomas>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PaulDThomas/boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/PaulDThomas/boilerplate/issues"
  },
  "homepage": ".",
  "keywords": [
    "react",
    "typescript",
    "boilerplate",
    "php",
    "api"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "parcel build public/index.html",
    "start": "parcel public/index.html",
    "prepare": "husky",
    "test": "jest --collectCoverage=true --maxWorkers=4",
    "test-watch": "jest --watch --collectCoverage=true"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "axios": "^1.1.3",
    "bootstrap": "^5.2.2",
    "node-sass": "^9.0.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.5.0",
    "react-bootstrap-icons": "^1.8.4",
    "react-dom": "^18.2.0",
    "sass": "^1.71.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "^2.8.2",
    "@parcel/transformer-sass": "^2.11.0",
    "@parcel/transformer-webmanifest": "^2.8.2",
    "@prettier/plugin-php": "^0.22.2",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.5",
    "@types/node": "^20.11.19",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "buffer": "^6.0.3",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.11",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-watch-typeahead": "^2.2.2",
    "lint-staged": "^15.2.2",
    "parcel": "^2.11.0",
    "prettier": "^3.2.5",
    "process": "^0.11.10",
    "ts-jest": "^29.0.3",
    "ts-jest-mock-import-meta": "^1.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
