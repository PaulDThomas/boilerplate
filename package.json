{
  "name": "boilerplate",
  "version": "0.2.0",
  "private": false,
  "description": "My typescript/php boiler plate.",
  "author": "Paul Thomas <@PaulDThomas>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PaulDThomas/boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/PaulDThomas/boilerplate/issues"
  },
  "homepage": ".",
  "keywords": [
    "react",
    "typescript",
    "boilerplate",
    "php",
    "api"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "parcel build public/index.html",
    "start": "parcel public/index.html",
    "prepare": "husky install",
    "test": "jest --watch",
    "test:coverage-report": "jest --coverage --watchAll=false"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "axios": "^1.1.3",
    "bootstrap": "^5.2.2",
    "node-sass": "^8.0.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.5.0",
    "react-bootstrap-icons": "^1.8.4",
    "react-dom": "^18.2.0",
    "sass": "^1.54.9",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "^2.8.2",
    "@parcel/transformer-sass": "^2.8.2",
    "@parcel/transformer-webmanifest": "^2.8.2",
    "@prettier/plugin-php": "^0.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.5",
    "@types/node": "^16.11.57",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "buffer": "^5.7.1",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-watch-typeahead": "^2.2.2",
    "lint-staged": "^13.0.3",
    "parcel": "^2.8.2",
    "prettier": "^2.7.1",
    "process": "^0.11.10",
    "ts-jest": "^29.0.3",
    "ts-jest-mock-import-meta": "^1.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
