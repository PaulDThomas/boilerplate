{
  "name": "boilerplate",
  "version": "0.3.1",
  "private": false,
  "description": "My typescript/php boiler plate.",
  "author": "Paul Thomas <@PaulDThomas>",
  "license": "MIT",
  "homepage": "https://github.com/PaulDThomas/boilerplate#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PaulDThomas/boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/PaulDThomas/boilerplate/issues"
  },
  "keywords": [
    "react",
    "typescript",
    "boilerplate",
    "php",
    "api"
  ],
  "files": [
    "dist"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "prepare": "husky",
    "start": "start.bat",
    "test": "jest --collectCoverage=true --maxWorkers=1",
    "test-watch": "jest --watch --collectCoverage=true --maxWorkers=4",
    "build": "parcel build public/index.html --public-url /ecrf/target/ --dist-dir public_html/ecrf/target/"
  },
  "dependencies": {
    "@asup/simple-table": "^1.3.13",
    "@fluentui/react-components": "^9.47.5",
    "@fluentui/react-icons": "^2.0.236",
    "axios": "^1.6.8",
    "lodash": "^4.17.21",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router": "^6.22.3",
    "react-router-dom": "^6.22.3"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "^2.8.2",
    "@parcel/transformer-webmanifest": "^2.8.2",
    "@prettier/plugin-php": "^0.22.2",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.5",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.11.19",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "buffer": "^6.0.3",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-jest": "^28.2.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.11",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-watch-typeahead": "^2.2.2",
    "lint-staged": "^15.2.2",
    "parcel": "^2.11.0",
    "prettier": "^3.2.5",
    "process": "^0.11.10",
    "ts-jest": "^29.0.3",
    "ts-jest-mock-import-meta": "^1.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "@parcel/transformer-js": {
    "inlineEnvironment": [
      "REACT_APP_*",
      "npm_package_*"
    ]
  },
  "@parcel/transformer-css": {
    "cssModules": {
      "pattern": "asup-[name]-[hash]-[local]",
      "dashedIndents": true
    }
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
